{
  "manifest_version": 2,
  "name": "Path of Exile Session ID finder",
  "author": "Duiker101",
  "description": "Find your own session ID quickly via browser button or context menu.",
  "version": "0.0.1",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "cookies",
    "contextMenus",
    "*://*.pathofexile.com/"
  ]
}